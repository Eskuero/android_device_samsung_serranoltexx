# Mount point	fs	device                                   flags
/system         ext4    /dev/block/mmcblk0p21
/cache          ext4    /dev/block/mmcblk0p22
/data           ext4    /dev/block/mmcblk0p24                    length=-16384
/efs            ext4    /dev/block/mmcblk0p10                    flags=display="EFS";backup=1
/modem          emmc    /dev/block/mmcblk0p1                     flags=display="Modem";backup=1
/external_sd    exfat   /dev/block/mmcblk1p1 /dev/block/mmcblk1  flags=display="Micro SDCard";storage;wipeingui;removable
/boot           emmc    /dev/block/mmcblk0p13
/recovery       emmc    /dev/block/mmcblk0p14
